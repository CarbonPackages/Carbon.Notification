{
    "name": "carbon/notification",
    "type": "neos-carbon",
    "description":
        "This package provides a tiny fusion helper for notifications.",
    "license": "GPL-3.0-or-later",
    "keywords": ["flow", "neos", "fusion", "helper", "carbon"],
    "require": {
        "neos/fusion": "^4.2 || ^5.0"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://jonnitto.ch",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Notification\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Carbon.Notification",
        "neos": {
            "package-key": "Carbon.Notification"
        }
    }
}
